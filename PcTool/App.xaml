<Application x:Class="PcTool.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:VM="clr-namespace:PcTool.ViewModel"
             xmlns:View="clr-namespace:PcTool.View"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
                
    	<ResourceDictionary>
    		<ResourceDictionary.MergedDictionaries>
    			<ResourceDictionary Source="Styles.xaml"/>
    		</ResourceDictionary.MergedDictionaries>
            
            <VM:MainViewModel x:Key="ViewModel"></VM:MainViewModel>
            <View:DictionaryPropertyGridValueConverter x:Key="PropertyGridVC"></View:DictionaryPropertyGridValueConverter>
    	</ResourceDictionary>
         
    </Application.Resources>
</Application>
